@model IList<BSP.Model.User>
@{
    ViewBag.Title = "用户状态管理";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<table class="table table-hover">
    <thead>
        <tr>
            <th># 用户姓名</th>
            <th># 用户状态</th>
            <th># 用户角色</th>
            <th># 操作</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            Html.RenderAction("UserStateListItem", item);
        }
    </tbody>
</table>
